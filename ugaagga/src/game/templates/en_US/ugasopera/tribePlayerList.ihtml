      <td>Rank</td>
      <td>Name</td>
      <td>&nbsp;</td>
      <td>Score</td>
      <td>Caves</td>
      <td>&nbsp;</td>
    </tr>
    <tmpl:ROWS>
    <tmpl:ROW>
    <tr align="center">
      <td>{rank}</td>
      <td><a href="{link}">{name}</a></td>
