<tmpl:DETAILS>
      <table class="box" cellpadding="20">
        <tr>
          <td align="center">
            <tmpl:LOGO>
              <img src="{logo}" width="{width}" height="{height}" border="0"><br>
            </tmpl:LOGO>
            &nbsp;
          </td>
        </tr>
        <tr>
          <td align="center">
            <span style="font-size: large">
              {tag}<br>
              {name}<br><br>
            </span >
            <tmpl:AWARD><a href="main.php?modus={award_modus}&amp;award={tag}"><img src="images/awards/{tag}_large.png" width="128" height="128" border="0" hspace="8"></a></tmpl:AWARD>
            <br><br>
            <a href="main.php?modus={playerList_modus}&tag={urltag}">Liste der Mitglieder</a><br><br>
            <a href="main.php?modus={tribeRelationList_modus}&tag={urltag}">&Uuml;bersicht &uuml;ber die Beziehungen</a><br><br>
            <a href="main.php?modus={tribeHistory_modus}&tag={urltag}">&Uuml;bersicht &uuml;ber die Stammesgeschichte</a><br><br>
            Anf&uuml;hrer: <a href="main.php?modus={playerDetail_modus}&detailID={playerID}">{leaderName}</a><br>
            Stellvertreter: <a href="main.php?modus={playerDetail_modus}&detailID={juniorLeaderID}">{juniorLeaderName}</a><br><br>

            <b>Beschreibung:</b><br><br>{description}<br><br>
          </td>
        </tr>
      </table>
</tmpl:DETAILS>
